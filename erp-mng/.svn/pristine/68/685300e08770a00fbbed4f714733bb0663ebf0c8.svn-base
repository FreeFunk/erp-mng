    <div class="layui-form-item"style="display: inline-block;width: 299px;height: 30px;">
        <label class="layui-form-label">车牌号:</label>
        <div class="layui-input-block">
            <input type="text" name="carPlateNum" readonly="readonly" placeholder="请输入车牌号" autocomplete="off" class="layui-input">
        </div>
    </div>
    <div class="layui-form-item"style="display: inline-block;width: 299px;height: 30px;">
        <label class="layui-form-label">车牌颜色:</label>
        <div id="carPaiColor" class="layui-input-block">
            <input type="text" name="carPlateColor"readonly="readonly"  placeholder="请输入车牌号" autocomplete="off" class="layui-input">
        </div>
    </div>
    <div class="layui-form-item"style="display: inline-block;width: 299px;height: 30px;">
        <label class="layui-form-label">车架号:</label>
        <div class="layui-input-block">
            <input type="text" name="carFrameNum" readonly="readonly" placeholder="请输入车架号" autocomplete="off" class="layui-input">
        </div>
    </div>
    <div class="layui-form-item layui-hide " style="display: inline-block;width: 299px;height: 30px;">
        <label class="layui-form-label">渠道代理ID</label>
        <div class="layui-input-block">
        </div>
    </div>
    <div class="layui-form-item"style="display: inline-block;width: 299px;height: 30px;">
        <label class="layui-form-label">渠道代理</label>
        <div class="layui-input-block">
            <input  readonly="readonly" type="text" id="channelAgentName" name="channelAgentName" class="layui-input">
        </div>
    </div>

    <div class="layui-form-item"style="display: inline-block;width: 299px;height: 30px;">
        <label class="layui-form-label">续费类型:</label>
        <div class="layui-input-block">
            <input type="text" name="isInstallXufei" readonly="readonly" autocomplete="off" class="layui-input">
        </div>
    </div>
            <div class="layui-form-item"style="display: inline-block;width: 299px;height: 30px;">
                <label class="layui-form-label">续费时间:</label>
                <div class="layui-input-block">
                    <input type="text" name="xufeiTime" id="xufeiTime"readonly="readonly" placeholder="请输入续费时间" autocomplete="off" class="layui-input">
                </div>
            </div>
    <div class="layui-form-item"style="display: inline-block;width: 299px;height: 30px;">
        <label class="layui-form-label">续费金额:</label>
        <div class="layui-input-block">
            <input type="text" name="xufeiMoney" readonly="readonly" placeholder="请输入续费金额" autocomplete="off" class="layui-input">
        </div>
    </div>


    <!--<div class="layui-form-item" style="display: inline-block;width: 299px;height: 30px;">-->
        <!--<label class="layui-form-label">是否前装续费</label>-->
        <!--<div id="isInstallXufeiId" class="layui-input-block">-->
            <!--<input type="text" name="isInstallXufei" readonly="readonly" placeholder="请输入备注" autocomplete="off" class="layui-input">-->
        <!--</div>-->
    <!--</div>-->
    <div class="layui-form-item layui-hide" style="display: inline-block;width: 299px;height: 30px;">
        <label class="layui-form-label">供应商ID</label>
        <div class="layui-input-block">
            <input type="text" name="supplierId" readonly="readonly" placeholder="请输入供应商ID" autocomplete="off" class="layui-input">
        </div>
    </div>
