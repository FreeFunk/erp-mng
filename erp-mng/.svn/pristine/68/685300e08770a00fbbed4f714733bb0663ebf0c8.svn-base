<script src="../../../../../layuiadmin/layui/layui.js"></script>
<script>
    layui.config({
        base: '../../../../../layuiadmin/' //静态资源所在路径
    }).extend({
        index: 'lib/index', //主入口模块
        $tool:'tool'
    }).use(['index', 'form','$tool','laydate'], function(){

        var $ = layui.$
            ,$tool = layui.$tool
            ,laydate = layui.laydate
            ,admin = layui.admin
            ,form = layui.form;

        getOrder = function() {
            var ownerUnlocationRemindId = $("#ownerUnlocationRemindId").val();
            debugger
            admin.req({
                url: layui.setter.basePath + '/admin/erpNotnolineCarInfo/erpNotnolineCarSendMessageInfo.jsn',
                data:{'ownerUnlocationRemindId':ownerUnlocationRemindId},
                method:"post",
                success:function (data) {
                    var erpUnlocationRemindMsgRec = data.data.ErpUnlocationRemindMsgRec;//拿到短信发送对象
                    //$("#id").val(erpUnlocationRemindMsgRec.id);
                    //车牌号 carPlateNum
                    $("#carPlateNum").val(erpUnlocationRemindMsgRec.carPlateNum);
                    //车架号 carFrameNum
                    $("#carFrameNum").val(erpUnlocationRemindMsgRec.carFrameNum);
                    //发送类型 sendType
                    $("#sendType").val(erpUnlocationRemindMsgRec.sendType);
                    //短信内容  msgText
                    $("#msgText").val(erpUnlocationRemindMsgRec.msgText);
                    //联系人  contactPerson
                    $("#contactPerson").val(erpUnlocationRemindMsgRec.contactPerson);
                    //联系电话  contactPhoneNum
                    $("#contactPhoneNum").val(erpUnlocationRemindMsgRec.contactPhoneNum);
                    //发送时间  sendTime
                    $("#sendTime").val(erpUnlocationRemindMsgRec.sendTime);
                    //发送状态  sendState
                    $("#sendState").val(erpUnlocationRemindMsgRec.sendState);
                },
            });
        }
        //getOrder();

        laydate.render({
            elem: '#createTime', //指定元素
            type:'datetime'
        });
        laydate.render({
            elem: '#sendTime', //指定元素
            type:'datetime'
        });
    })
</script>
</body>
</html>