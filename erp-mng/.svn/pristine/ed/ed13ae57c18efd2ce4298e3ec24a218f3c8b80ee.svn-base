                        roadTransportNum: "道路运输证号",provinceName: "所属省",cityName: '所属市', xianquName: "所属县",
                        yehuPerson: "车主/业户",contactPerson:"联系人",contactPhoneNum:"联系人手机",phoneVerificationState:"手机验证状态",firstOnlineTime:"首次上线时间",carFrameNum:"车辆识别代码/车架号"
                        ,carPlateNum:"车牌号",carPlateColor:"车牌颜色",null:"燃料种类",null:"车辆类型",null:"车辆品牌"
                        ,phoneVerificationState:"总质量(kg)",phoneVerificationState:"核定载质量(kg)",phoneVerificationState:"准牵引总质量(kg)"
                        ,phoneVerificationState:"外廓尺寸-长(mm)(kg)",phoneVerificationState:"外廓尺寸-宽(mm)",phoneVerificationState:"外廓尺寸-高(mm)"
                        ,phoneVerificationState:"货厢内部尺寸-长(mm)",phoneVerificationState:"货厢内部尺寸-宽(mm)",phoneVerificationState:"货厢内部尺寸-高(mm)"
                        ,phoneVerificationState:"轴数",phoneVerificationState:"轮胎数"
                        ,phoneVerificationState:"轮胎规格",phoneVerificationState:"车辆出厂日期",phoneVerificationState:"经营范围"
                        ,phoneVerificationState:"车身颜色",phoneVerificationState:"道路运输经营许可证号",phoneVerificationState:"车辆购置方式",
                        phoneVerificationState:"车辆保险到期时间",phoneVerificationState:"检验有效期至",phoneVerificationState:"执照上传数"
                        ,phoneVerificationState:"服务合同到期时间",phoneVerificationState:"行驶证发证日期",phoneVerificationState:"发动机号"
                        ,phoneVerificationState:"发动机型号",beidouOperator:"北斗平台"});

                    excel.exportExcel(data, '已删除车辆信息.xlsx', 'xlsx');
                    $("#carInfoExcelOn").html("导出车辆信息");
                    $("#carInfoExcelOn").attr("class","layui-btn layui-btn-warm");
                    layer.close(loading);
