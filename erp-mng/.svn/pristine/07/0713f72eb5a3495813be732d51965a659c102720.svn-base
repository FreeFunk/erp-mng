<script src="../../../../../layuiadmin/layui/layui.js"></script>
<script>
        layui.config({
            base: '../../../../../layuiadmin/' //静态资源所在路径
        }).extend({
            index: 'lib/index', //主入口模块
            $tool:'tool'
        }).use(['index', 'form','$tool','laydate'], function(){

            var $ = layui.$
                ,$tool = layui.$tool
                ,laydate = layui.laydate
                ,admin = layui.admin
                ,form = layui.form;

            /*var queryArgs = $tool.getQueryParam();//获取查询参数
            var id = queryArgs['id'];*/
            //监听修改保存
            form.on('submit(ErpCarInfo-form-edit)', function(data){
                var xianquId =  $("#xianquName").val();
                data.field.xianquId=xianquId;//县区id

                var ownerTeamName =  $("#ownerTeamName").val();
                data.field.ownerTeamName=ownerTeamName;//车队名

                var supplierId =  $("#supplierName").val();
                data.field.supplierId=supplierId;//供应商id

                var channelAgentId =  $("#channelAgentName").val();
                data.field.channelAgentId=channelAgentId;//渠道代理id

                var yewuUserId =  $("#yewuUserName").val();
                data.field.yewuUserId=yewuUserId;//业务人员id

                var installUserId =  $("#installUserName").val();
                data.field.installUserId=installUserId;//安装人员id

                var inputUserId =  $("#inputUserName").val();
                data.field.inputUserId=inputUserId;//录入人员id

                var getMoneyType =  $("#getMoneyType").val();
                data.field.getMoneyType=getMoneyType;//收款方式

                var val_terminalInstallType = $('#InstallType input[name="terminalInstallType"]:checked ').val();//安装类型
                data.field.terminalInstallType = val_terminalInstallType;

                var carPlateColor = $('#carPaiColor input[name="carPlateColor"]:checked ').val();//车辆颜色
                data.field.carPlateColor = carPlateColor;

                // var beidouOperator =  $("#beidouOperator").val();
                // data.field.beidouOperator=beidouOperator;//北斗id
                var field = data.field; //获取提交的字段

                var index = parent.layer.getFrameIndex(window.name);//先得到当前iframe层的索引
                admin.req({
                    url:layui.setter.basePath +'/admin/erpCarInfo/saveOrUpdate.jsn',
                    data:field,
                    method:"post",
                    success:function (data) {
                        parent.layui.table.reload('ErpCarInfo-list'); //重载表格
                        parent.layer.close(index); //再执行关闭
                        parent.layer.msg('修改成功', {
                            icon: 1
                        });
                    }
                });
            });



            laydate.render({
                elem: '#createTime', //指定元素
                type:'datetime'
            });
            laydate.render({
                elem: '#endTime', //指定元素
                type:'datetime'
            });
            laydate.render({
                elem: '#lastXufeiTime', //指定元素
                type:'datetime'
            });

            laydate.render({
                elem: '#installationTime', //指定元素
                type:'datetime'
            });

            //付款状态
            function payState() {
                var payStateInput = $("#payStateInput").val();
                if(payStateInput==0){
                    $("#payState").val(payStateInput);
                    layui.form.render('select');//重新渲染表格
                }else if (payStateInput==1) {
                    $("#payState").val(payStateInput);
                    layui.form.render('select');//重新渲染表格
                }
            }
            payState();

            //付款状态
            function getMoneyType() {
                var getMoneyTypeInput = $("#getMoneyTypeInput").val();
                if(getMoneyTypeInput==0){
                    $("#getMoneyType").val(getMoneyTypeInput);
                    layui.form.render('select');//重新渲染表格
                }else if (payStateInput==1) {
                    $("#getMoneyType").val(getMoneyTypeInput);
                    layui.form.render('select');//重新渲染表格
                }else if (payStateInput==2) {
                    $("#getMoneyType").val(getMoneyTypeInput);
                    layui.form.render('select');//重新渲染表格
                }
            }
            getMoneyType();


            //查询所有车队
            function ownerTeamNameList() {
                var ownerTeamNameInput = $("#ownerTeamNameInput").val();
                admin.req({
                    url:layui.setter.basePath + '/admin/erpCarInfo/selectCarTeam.jsn',
                    method:"post",
                    data:{} ,
                    success:function(data){
                        var catTeam = data.data;
                        //将查询到的角色列表展示到下拉框中去;
                        if(catTeam!="undefined"){
                            for(var i=0;i<catTeam.length;i++){
                                var s = catTeam[i].id+","+catTeam[i].carTeamName
                                $("#ownerTeamName").append("<option value="+s+">"+catTeam[i].carTeamName+"</option>");
                                if(s==ownerTeamNameInput){
                                    $("#ownerTeamName").val(s);
                                }
                                layui.form.render('select');//重新渲染表格
                            }
                        }
                    }
                });
            }
            ownerTeamNameList();

            //查询所有地区
            function addressList() {
                var xianquNameInput = $("#xianquNameInput").val();
                admin.req({
                    url:layui.setter.basePath + '/admin/erpCarTeam/listAllXianQu.jsn',
                    method:"post",
                    data:{} ,
                    success:function(data){
                        var xianQu = data.data;
                        //将查询到的角色列表展示到下拉框中去;
                        if(xianQu!="undefined"){
                            for(var i=0;i<xianQu.length;i++){
                                var s = xianQu[i].id+","+xianQu[i].name
                                $("#xianquName").append("<option value="+s+">"+xianQu[i].name+"</option>");
                                if(s==xianquNameInput){
                                    $("#xianquName").val(s);
                                }
                                layui.form.render('select');//重新渲染表格
                            }
                        }
                    }
                });
            }
            addressList();

            //渠道代理
            function channelAgentNameList() {
                var channelAgentNameInput = $("#channelAgentNameInput").val();
                admin.req({
                    url:layui.setter.basePath + '/admin/erpCarInfo/listAllChannelAgentNameList.jsn',
                    method:"post",
                    data:{} ,
                    success:function(data){
                        var channelAgentName = data.data;
                        //将查询到列表展示到下拉框中去;
                        if(channelAgentName!="undefined"){
                            for(var i=0;i<channelAgentName.length;i++){
                                var s = channelAgentName[i].id+","+channelAgentName[i].channelAgentName
                                $("#channelAgentName").append("<option value="+s+">"+channelAgentName[i].channelAgentName+"</option>");
                                if(s==channelAgentNameInput){
                                    $("#channelAgentName").val(s);
