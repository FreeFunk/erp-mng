<script src="../../../../../layuiadmin/layui/layui.js"></script>
<script>
        layui.config({
            base: '../../../../../layuiadmin/' //静态资源所在路径
        }).extend({
            index: 'lib/index' //主入口模块
        }).use(['index', 'form','laydate'], function(){
            var $ = layui.$
                ,laydate = layui.laydate
                ,admin  = layui.admin
                ,form = layui.form;

            //监听提交
            form.on('submit(ErpCarInfo-form-submit)', function(data){
                var xianquId =  $("#xianquName").val();
                data.field.xianquId=xianquId;//县区id

                var ownerTeamName =  $("#ownerTeamName").val();
                data.field.ownerTeamName=ownerTeamName;//车队名

                var supplierId =  $("#supplierName").val();
                data.field.supplierId=supplierId;//供应商id

                var channelAgentId =  $("#channelAgentName").val();
                data.field.channelAgentId=channelAgentId;//渠道代理id

                var yewuUserId =  $("#yewuUserName").val();
                data.field.yewuUserId=yewuUserId;//业务人员id

                var installUserId =  $("#installUserName").val();
                data.field.installUserId=installUserId;//安装人员id

                // var beidouOperator =  $("#beidouOperator").val();
                // data.field.beidouOperator=beidouOperator;//北斗id

                var getMoneyType =  $("#getMoneyType").val();
                data.field.getMoneyType=getMoneyType;//收款方式

                var val_terminalInstallType = $('#InstallType input[name="terminalInstallType"]:checked ').val();//安装类型
                data.field.terminalInstallType = val_terminalInstallType;

                var carPlateColor = $('#carPaiColor input[name="carPlateColor"]:checked ').val();//车辆颜色
                data.field.carPlateColor = carPlateColor;

                var field = data.field; //获取提交的字段
                var index = parent.layer.getFrameIndex(window.name); //先得到当前iframe层的索引
                //提交 Ajax 成功后，关闭当前弹层并重载表格
                admin.req({
                    url:layui.setter.basePath +'/admin/erpCarInfo/saveOrUpdate.jsn',
                    data:field,
                    method:"post",
                    success:function (data) {
                        debugger
                        if(data.success){
                            parent.layui.table.reload('ErpCarInfo-list'); //重载表格
                            parent.layer.close(index); //再执行关闭
                            parent.layer.msg('添加成功', {
                                icon: 1
                            });
                        } else if(data.data.err){
                            layer.alert('没有SIM卡，请先添加SIM卡!', {
                                offset: 'auto',
                            });
                        }else if (data.data.err1){
                            layer.alert('该sim卡已使用,请更换卡号!', {
                                offset: 'auto',
                            });
                        }else if (data.data.err2){
                            layer.alert('终端编号不存在,查看是否输入正确', {
                                offset: 'auto',
                            });
                        }
                    },
                });
            });

            laydate.render({
                elem: '#createTime', //指定元素
                type:'datetime'
            });
            laydate.render({
                elem: '#endTime', //指定元素
                type:'datetime'
            });
            laydate.render({
                elem: '#lastXufeiTime', //指定元素
                type:'datetime'
            });
            laydate.render({
                elem: '#installationTime', //指定元素
                type:'datetime'
            });

            //查询所有车队
            function ownerTeamNameList() {
                admin.req({
                    url:layui.setter.basePath + '/admin/erpCarInfo/selectCarTeam.jsn',
                    method:"post",
                    data:{} ,
                    success:function(data){
                        var catTeam = data.data;
                        //将查询到的角色列表展示到下拉框中去;
                        if(catTeam!="undefined"){
                            for(var i=0;i<catTeam.length;i++){
                                $("#ownerTeamName").append("<option value="+catTeam[i].id+">"+catTeam[i].carTeamName+"</option>");
                                layui.form.render('select');//重新渲染表格
                            }
                        }
                    }
                });
            }
            ownerTeamNameList();

            //查询所有地区
            function addressList() {
                admin.req({
                    url:layui.setter.basePath + '/admin/erpCarTeam/listAllXianQu.jsn',
                    method:"post",
                    data:{} ,
                    success:function(data){
                        var xianQu = data.data;
                        //将查询到的角色列表展示到下拉框中去;
