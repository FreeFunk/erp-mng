            //查询SIM卡
            function simList() {
                admin.req({
                    url:layui.setter.basePath + '/admin/erpCarInfo/simList.jsn',
                    method:"post",
                    data:{} ,
                    success:function(data){
                        var simArr = data.data;
                        //将查询到列表展示到下拉框中去;
                        if(simArr!="undefined"){
                            for(var i=0;i<simArr.length;i++){
                                $("#simCode").append("<option value="+simArr[i].simNum+">"+simArr[i].simNum+"</option>");
                                layui.form.render('select');//重新渲染表格
                            }
                        }
                    }
                });
            }
            simList();




        })

</script>
</body>
</html>